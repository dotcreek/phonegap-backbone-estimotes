<header class="bar bar-nav">
    <button class="btn btn-link btn-back btn-nav pull-left">
        <span class="icon icon-left-nav"></span>
        Back
    </button>
    <h1 class="title"><%= model.get('description') %></h1>
</header>
<div class="content">
    <ul class="table-view">
        <li class="table-view-cell media">
            <div class="media-body">
                <% model.get('events').forEach(function (event) { %>
                    <% if (event.id === model._previousAttributes.eventId) {%>
                        <h4 class="room-name">
                            <%= event.room.name%>
                        </h4>
                        <p class="content-date">
                            <%- App.utils.convertDate([event.startAt, event.endAt]) %>
                        </p>
                    <% } %>
                <% })%>

                <%=model.get( 'content') %>
            </div>
        </li>
    </ul>
</div>
