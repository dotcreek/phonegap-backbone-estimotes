<header class="bar bar-nav">
    <a class="icon icon-left-nav pull-left btn-back"></a>
    <a class="icon <%= model.isFavorite() %> pull-right" id="add-favorite"></a>
    <h1 class="title truncated"><%- model.get('description') %></h1>
</header>

<div class="content">
    <ul class="table-view">
        <li class="table-view-cell media">
            <div class="media-body">
                <h4 class="room-name">
                    <%- model.get('events')[0].room.name %>
                </h4>
                <p class="content-date">
                    <%- App.utils.convertDate([model.get('events')[0].startAt, model.get('events')[0].endAt],'times') %>
                </p>
                <p>
                    <%= model.get('content') %>
                </p>
            </div>
        </li>
    </ul>
</div>
