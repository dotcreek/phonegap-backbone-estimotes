<%= JST['app/scripts/templates/partials/_navigation.ejs']({
    nav: 'favorites',
    title: 'utils.favorites'
}) %>

<div class="content">
    <ul class="table-view rooms-home">
        <% collection.forEach(function(item){ %>
            <li class="table-view-cell media navigate-right">
                <a href='#contents/<%- item.get("favoriteId")%>/<%- item.get("eventId")%>'>
                    <div class="media-body">
                        <div class="room-content-full">
                            <span class="content-hour"> <i class="fa fa-calendar"></i>
                                <%- App.utils.convertDate(item.get('events')[0].startAt, 'day') %>
                                <%- App.utils.convertDate(item.get('events')[0].startAt, 'month') %>
                                <%- App.utils.convertDate([item.get('events')[0].startAt, item.get('events')[0].endAt],'times') %>
                            </span>
                            <h4 class="content-title">
                                <%- item.get('description') %>
                            </h4>
                        </div>
                    </div>
                </a>
            </li>
        <% })%>
    </ul>
</div>
