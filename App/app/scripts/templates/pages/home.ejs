<%= JST['app/scripts/templates/partials/_navigation.ejs']({
    nav: 'home',
    title: 'utils.home'
}) %>

<div class="content">
    <ul class="table-view rooms-home">
        <% collection.forEach(function(item){ %>
            <li class="table-view-cell media">
                <a href='#contents/<%- item.get("contentId")%>/<%- item.get("id")%>'>
                    <div class="media-body">
                        <h4 class="content-title">
                            <%- item.get('content').description %>
                        </h4>
                        <h4 class="room-name">
                            <%- item.get('room').name %>
                        </h4>
                        <p class="content-date">
                            <%- App.utils.convertDate([item.get('startAt'), item.get('endAt')]) %>
                        </p>
                    </div>
                </a>
            </li>
        <% })%>
    </ul>
</div>
